<div class="content-wrapper">
  <div class="content-header">
    <div class="container-fluid">
      <div class="row mb-2">
        <div class="col-sm-6">
          <h1 class="m-0 text-dark">Lista de Departamentos</h1>
        </div>
      </div>
    </div>
  </div>
</div>
<section>
  <p id="notice"><%= notice %></p>
  <p>
    <strong>Name:</strong>
    <%= @departament.name %>
  </p>
  <p>
    <strong>Manager:</strong>
    <%= @departament.manager %>
  </p>
  <p>
    <strong>Telephone:</strong>
    <%= @departament.telephone %>
  </p>
  <%= link_to 'Edit', edit_departament_path(@departament) %> |
  <%= link_to 'Back', departaments_path %>
</section>
